@model IEnumerable<DonareISS.Pacient>

@{
    ViewBag.Title = "Pacienti";
    Layout = "~/Views/Shared/_LayoutMedic.cshtml";
}

<h2>Pacienti</h2>

@foreach (var item in Model)
{
    <div class="row card">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <p>
                @Html.DisplayNameFor(model => model.CantitateCeruta)
                @Html.DisplayFor(modelItem => item.CantitateCeruta)
            </p>
        </div>
    </div>
}